Grammar

    0 $accept: prog $end

    1 prog: header var_section body_section

    2 header: PROGRAMM STRING ';' ENDLS

    3 var_section: VAR var_lines

    4 var_lines: var_line
    5          | var_lines var_line

    6 var_line: vars ':' FUNCTION '(' args ')' ';' ENDLS
    7         | vars ':' DOUBLETYPE ';' ENDLS
    8         | vars ':' DOMAIN ';' ENDLS
    9         | vars ':' INTTYPE ';' ENDLS

   10 args: STRING
   11     | args ',' STRING

   12 vars: STRING
   13     | vars ',' STRING

   14 body_section: BEGIN body_lines footer

   15 body_lines: body_line
   16           | body_lines body_line

   17 body_line: STRING ":=" FUNCTIONVAR ';' ENDLS
   18          | STRING ":=" DOUBLEVAR ';' ENDLS
   19          | STRING ":=" INTVAR ';' ENDLS
   20          | STRING DOML ":=" DOUBLEVAR ';' ENDLS
   21          | STRING DOMR ":=" DOUBLEVAR ';' ENDLS
   22          | STRING ":=" STRING '(' STRING ',' STRING ',' STRING ',' STRING ',' STRING ')' ';' ENDLS

   23 footer: END ENDLS

   24 ENDLS: ENDL
   25      | ENDLS ENDL


